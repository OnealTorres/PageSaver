{
  "manifest_version": 2,

  "name": "PageSaver",
  "description": "An extension to save the your pages.",
  "version": "1.0",
  "icons": {
    "16": "./images/icon16.png",
    "32": "./images/icon32.png",
    "48": "./images/icon48.png",
    "128": "./images/icon128.png"
  },
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "./images/icon128.png",
    "default_title": "PageSaver",
    "default_popup": "popup.html"
  }
}
